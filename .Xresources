URxvt.boldFont:         xft:Liberation Mono for Powerline:bold:autohint=true:size=12
URxvt.italicFont:       xft:Liberation Mono for Powerline:italic:autohint=true:size=12
URxvt.boldItalicfont:   xft:Liberation Mono for Powerline:bold:italic:autohint=true:size=12

URxvt.perl-ext-common:      default,keyboard-select,url-select,clipboard
URxvt.url-select.launcher:  /home/kofish/.bin/launch_browser
URxvt.url-select.autocopy:  true
URxvt.url-select.button:    2
URxvt.url-select.underline: true
URxvt.clipboard.copycmd:    parcellite
URxvt.clipboard.pastecmd:    parcellite
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.keysym.M-c:           perl:clipboard:copy
URxvt.keysym.M-v:           perl:clipboard:paste
URxvt.keysym.M-C-v:         perl:clipboard:paste_escaped
URxvt.mapAlert:             true
URxvt.visualBell:           true
URxvt.urgentOnBell:         true

URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:	3
URxvt.tabbed.tab-bg:	0

